    vec3 side[] = {
        vertexInterp(cube[0], scalVal(cube[0]), cube[1], scalVal(cube[1])),
        vertexInterp(cube[1], scalVal(cube[1]), cube[2], scalVal(cube[2])),
        vertexInterp(cube[2], scalVal(cube[2]), cube[3], scalVal(cube[3])),
        vertexInterp(cube[3], scalVal(cube[3]), cube[0], scalVal(cube[0])),

        vertexInterp(cube[4], scalVal(cube[4]), cube[5], scalVal(cube[5])),
        vertexInterp(cube[5], scalVal(cube[5]), cube[6], scalVal(cube[6])),
        vertexInterp(cube[6], scalVal(cube[6]), cube[7], scalVal(cube[7])),
