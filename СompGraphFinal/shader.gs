float isolevel = 10.0f;

float scalVal(vec3 pos) {
    // float x1 = 0;
    float x1 = 0.5 * (cos(time) + 1.0) / 2.0;
    float x2 = -0.5 * (cos(time) + 1.0) / 2.0;
    return 1.0f / ((pos.x - x1) * (pos.x - x1) + pos.y * pos.y + pos.z * pos.z + 0.1) + 
           1.0f / ((pos.x - x2) * (pos.x - x2) + pos.y * pos.y + pos.z * pos.z + 0.1); 
}


int getCubeIndex(vec3 pos) {
    
    vec3 cube[] = {
        vec3(pos.x,     pos.y,      pos.z),
        vec3(pos.x + a, pos.y,      pos.z),
        vec3(pos.x + a, pos.y + a,  pos.z),
        vec3(pos.x,     pos.y + a,  pos.z),
        vec3(pos.x,     pos.y,      pos.z + a),
        vec3(pos.x + a, pos.y,      pos.z + a),
        vec3(pos.x + a, pos.y + a,  pos.z + a),
        vec3(pos.x,     pos.y + a,  pos.z + a)
    };

    int cubeIndex = 0;
    for (int i = 0; i < 8; i++) {
        if (scalVal(cube[i]) > isolevel) {
            cubeIndex |= (1 << i);
        }
    }
    return cubeIndex;
}

vec3 vertexInterp(vec3 v0, float l0, vec3 v1, float l1){ 
  return mix(v0, v1, (isolevel - l0) / (l1 - l0)); 
}


void main() {
    vec3 pos = gl_in[0].gl_Position.xyz;

    vec3 cube[] = {
    vec3(pos.x,     pos.y,      pos.z),
    vec3(pos.x + a, pos.y,      pos.z),
    vec3(pos.x + a, pos.y + a,  pos.z),
