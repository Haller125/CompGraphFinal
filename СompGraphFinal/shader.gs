        vertexInterp(cube[3], scalVal(cube[3]), cube[7], scalVal(cube[7])),
    };


    int cubeindex = getCubeIndex(pos);

   /* Cube is entirely in/out of the surface */
   if (edgeTable[cubeindex] == 0)
      return;

    int edges[] = triTable[cubeindex];
    
    for (int i = 0; edges[i] != -1; i += 3) {
        vec3 A = side[edges[i]];
        vec3 B = side[edges[i + 1]];
        vec3 C = side[edges[i + 2]];

        
        vec3 AB = B - A;
        vec3 AC = C - A;
        vec3 normalNotNorm = cross(AB, AC);
        normal = normalize(normalNotNorm);

        gl_Position = projection * view * model * vec4(A, 1.0);

       

        FragPos = vec3(model * vec4(A, 1.0));

        EmitVertex();
        gl_Position = projection * view * model * vec4(B, 1.0);
        FragPos = vec3(model * vec4(B, 1.0));
        EmitVertex();

        gl_Position = projection * view * model * vec4(C, 1.0);
        FragPos = vec3(model * vec4(C, 1.0));
        EmitVertex();
        EndPrimitive();
    }

      
